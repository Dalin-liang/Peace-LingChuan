{
  "extends": "eslint-config-egg/typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
    "no-console": "off",
    "no-irregular-whitespace": "off",
    "array-bracket-spacing": [
      1,
      "never"
    ]
  },
  "plugins": [
    "@typescript-eslint"
  ]
}